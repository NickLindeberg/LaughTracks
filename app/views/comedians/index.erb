<h1>All Comedians</h1>

<% @comedians.each do |c| %>
  <ul>
    <li>Comedian: <%= c.name %></li>
    <li>Age: <%= c.age %></li>
    <li>City: <%= c.city %>
    <li>Specials: <% c.specials.each do |special| %>
      <li><%= special.name %></li>
      <li><%= special.run_time %></li>
      <li><%= special.image_url %></li>
      <br>
    <% end %>
  </ul>

<% end %>
